{
  "name": "libav-stream-info-wasm",
  "version": "1.0.0",
  "description": "wasm bindings for libav to extract audio tags",
  "main": "dist/ffprobe-core.js",
  "types": "types.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streamonkey/libav-stream-info-wasm.git"
  },
  "keywords": [
    "ffprobe",
    "WebAssembly",
    "wasm",
    "ffmpeg",
    "libav",
    "ID3",
    "Metadata",
    "audio"
  ],
  "author": "Wilhelm Bartel <wilhelm.bartel@streamonkey.de>",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/streamonkey/libav-stream-info-wasm/issues"
  },
  "homepage": "https://github.com/streamonkey/libav-stream-info-wasm#readme",
  "devDependencies": {
    "@types/emscripten": "^1.39.6"
  },
  "files": [
    "dist",
    "types.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/ffprobe-core.js",
